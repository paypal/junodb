<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Juno All Test Suite" parallel="false" verbose="1">
	<test verbose="1" name="Juno Integration Test" annotations="JDK">
		<packages>
			<package name="com.paypal.qa.juno.*">
				<exclude name="com.paypal.qa.juno.mthreadload"></exclude>
				<exclude name="com.paypal.qa.juno.interop"></exclude>
			</package>
			<package name="com.paypal.qa.juno.usf.*">
				<exclude name="com.paypal.qa.juno.usf.JunoUSFFactoryRChild"></exclude>
			</package>
		</packages>

		<classes>
			<class name="com.paypal.qa.juno.BatchCreateTest"/>
			<class name="com.paypal.qa.juno.BatchDestroyTest"/>
			<class name="com.paypal.qa.juno.BatchGetTest"/>
			<class name="com.paypal.qa.juno.BatchSetTest"/>
			<class name="com.paypal.qa.juno.BatchUpdateTest"/>
			<class name="com.paypal.qa.juno.BatchReactCreateTest"/>
			<class name="com.paypal.qa.juno.BatchReactDestroyTest"/>
			<class name="com.paypal.qa.juno.BatchReactGetTest"/>
			<class name="com.paypal.qa.juno.BatchReactSetTest"/>
			<class name="com.paypal.qa.juno.BatchReactUpdateTest"/>
			<class name="com.paypal.qa.juno.ConditionalUpdateTest"/>
			<class name="com.paypal.qa.juno.ConnectionPoolingTest"/>
			<class name="com.paypal.qa.juno.CreateTest"/>
			<class name="com.paypal.qa.juno.DestroyTest"/>
			<class name="com.paypal.qa.juno.GetTest"/>
			<class name="com.paypal.qa.juno.SetTest"/>
			<class name="com.paypal.qa.juno.Juno20ClientConfigurationTest"/>
			<class name="com.paypal.qa.juno.MetricTest"/>
			<class name="com.paypal.qa.juno.ConnectionPoolingTest"/>
			<class name="com.paypal.qa.juno.UpdateTest"/>
			<class name="com.paypal.qa.juno.ReactorCreateTest"/>
			<class name="com.paypal.qa.juno.ReactorDestroyTest"/>
			<class name="com.paypal.qa.juno.ReactorGetTest"/>
			<class name="com.paypal.qa.juno.ReactorSetTest"/>
			<class name="com.paypal.qa.juno.ReactorConditionUpdateTest"/>
			<class name="com.paypal.qa.juno.usf.JunoUSFFactoryClientTest"/>
			<class name="com.paypal.qa.juno.usf.JunoUSFFactoryChildTest"/>
			<class name="com.paypal.qa.juno.usf.JunoUSFFactoryMultiClientTest"/>
		</classes>
	</test>
</suite>
